     border:none;
    background-image: none;
}

/************************** Data view****************/
.x-portlet .x-panel-body{
    background: white;
    font: 10px Arial, Helvetica, sans-serif;
}
.x-portlet .thumb{
    /*background: #dddddd;
    padding: 3px;*/
}

.x-quirks .x-portlet .thumb {
    padding-bottom: 3px;
}

.x-portlet .thumb img{
    height: 50px;
    width: 50px;
}
.x-portlet .thumb-wrap{
    float: left;
    margin: 4px;
    margin-right: 0;
    padding: 3px;
    height:85px;
}
.x-portlet .thumb-wrap span {
    
    display: block;
    overflow: hidden;
    text-align: center;
    width: 60px; /* for ie to ensure that the text is centered */
}

.x-portlet .x-item-over{
    border:1px solid #dddddd;
    background: #efefef url(../images/over.gif) repeat-x left top;
    padding: 4px;
}

.x-portlet .x-item-selected{
    background: #eff5fb url(../images/selected.gif) no-repeat right bottom;
    border:1px solid #99bbe8;
    padding: 4px;
}
.x-portlet .x-item-selected .thumb{
    background:transparent;
}

.x-portlet .loading-indicator {
    font-size:11px;
    background-image:url('../../extjs/resources/themes/images/default/grid/loading.gif');
    background-repeat: no-repeat;
    background-position: left;
    padding-left:20px;
    margin:10px;
}

.x-view-selector {
    position:absolute;
    left:0;
    top:0;
    width:0;
    border:1px dotted;
    opacity: .5;
    -moz-opacity: .5;
    filter:alpha(opacity=50);
    zoom:1;
    background-color:#c3daf9;
    border-color:#3399bb;
}
.ext-strict .ext-ie .x-tree .x-panel-bwrap{
    position:relative;
    overflow:hidden;
}
.rowcontent {
    
    background:#fdffc9 !important;
    font-size: small;
    color: green;
    font-weight: bolder;
}

.rowcontent1 {
    
    background:#fdffc9 !important;
    font-size: medium;
    color: black;
}
.x-grid-row-alt {
background: red !important;
}

